{
  "rules": {
    "prod": {
      ".read": "root.child('user_access/admins/'+auth.uid).exists()",
      ".write": "root.child('user_access/admins/'+auth.uid).exists()",
      "chatMessage": {
        ".read": true,
        ".write": "auth !== null"
      },
      "controls": {
        "admins": {
          ".read": "root.child('user_access/admins/'+auth.uid).exists()",
          ".write": "root.child('user_access/admins/'+auth.uid).exists()"
        },
        "djs": {
          ".read": "root.child('user_access/djs/'+auth.uid).exists()",
          ".write": "root.child('user_access/djs/'+auth.uid).exists()"
        },
        "users": {
          ".read": "auth !== null",
          ".write": "auth !== null"
        },
        "command": {
          ".read": true,
          ".write": true
        }
      },
      "news": {
        ".read": true,
        ".write": true
      },
      "nowPlaying": {
        ".read": true,
        ".write": "root.child('user_access/djs/'+auth.uid).exists()"
      },
      "playlists": {
        ".read": true,
        ".write": "root.child('user_access/djs/'+auth.uid).exists()"
      },
      "queue": {
        ".read": true,
        ".write": "root.child('user_access/djs/'+auth.uid).exists()"
      },
      "schedule": {
        ".read": true,
        ".write": "root.child('user_access/djs/'+auth.uid).exists()"
      },
      "songs": {
        ".read": true,
        ".write": "root.child('user_access/djs/'+auth.uid).exists()"
      },
      "user_access": {
        ".read": true,
        ".write": "root.child('user_access/admins/'+auth.uid).exists()"
      }
    }
  }
}
